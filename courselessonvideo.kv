MDScreen:
    name: "courselessonvideo"
    MDFloatLayout:
        MDTopAppBar:
            md_bg_color: rgba(254,157,182,255)
            pos_hint: {"center_x":0.5,"center_y":0.95}
        MDLabel:
            id: lessonname
            text: "  "
            font_name: "DPoppins"
            font_size: "20sp"
            pos_hint: {"center_x":0.7,"center_y":0.94}
            theme_text_color: "Custom"
            text_color: rgba(222, 254, 237,255)
        MDIconButton:
            icon : "arrow-left"
            pos_hint : {"center_y" :0.95}
            user_font_size: "30sp"
            theme_text_color: "Custom"
            text_color : rgba(255,255,255,255)
            on_release:
                root.manager.transition.direction = 'left'
                root.manager.current = 'course1videos'
        MDLabel:
            id: videourl
            text: " "
            pos_hint:{"center_x":0.5,"center_y":0.5}